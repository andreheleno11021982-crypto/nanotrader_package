{
  "expo": {
    "name": "NanoTrader",
    "slug": "nanotrader",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "package": "com.nanotrader.app",
      "versionCode": 1
    },
    "ios": {
      "bundleIdentifier": "com.nanotrader.app",
      "supportsTablet": true
    },
    "extra": {
      "eas": {
        "projectId": "nanotrader-projeto"
      }
    }
  }
}
